<h2>Projects</h2><hr />

<table class="table table-bordered">
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Course</th>
    </tr>
    @foreach (var item in ViewBag.pr) { 
    <tr>
        <td>@item.Title</td>
        <td>@item.Desc</td>
        <td>@item.Course</td>
    </tr>
    }
</table>
